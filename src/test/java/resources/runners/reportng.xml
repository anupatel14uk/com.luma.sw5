<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1">
    <listeners>
        <listener class-name="com.luma.customlisteners.CustomListeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="Gear Test">
        <classes>
            <class name="com.luma.testsuite.GearTest"></class>
        </classes>
    </test>
    <test name="Men Test" >
        <classes>
            <class name="com.luma.testsuite.MenTest"/>
        </classes>
    </test>
    <test name="Women Test" >
        <classes>
            <class name="com.luma.testsuite.WomenPageTest"/>
        </classes>
    </test>
</suite>